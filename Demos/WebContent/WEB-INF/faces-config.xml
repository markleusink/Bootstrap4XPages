<?xml version="1.0" encoding="UTF-8"?>
<faces-config>
  <!-- bean used for the select2 demo -->
  <managed-bean>
    <managed-bean-name>namePickerBean</managed-bean-name>
    <managed-bean-scope>view</managed-bean-scope>
    <managed-bean-class>com.bootstrap4xpages.NamesDataStore</managed-bean-class>
  </managed-bean>
  <!-- phase listener for the alerts demo -->
  <lifecycle>
    <phase-listener>com.bootstrap4xpages.MultiPageMessages</phase-listener>
  </lifecycle>
  <managed-bean id="dBar">
    <managed-bean-name>dBar</managed-bean-name>
    <managed-bean-class>eu.linqed.debugtoolbar.DebugToolbar</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
</faces-config>
